CREATE TABLE tekken_girls (
    PersonID int,
    CharacterName varchar(255),
    FightingStyle varchar(255),
    Origin varchar(255),
    Eyes varchar (225),
    Hair varchar (255),
    GameAppearance varchar(255)
);

